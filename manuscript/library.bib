  keywords = {Adult,Aged,{Aged, 80 and over},{Aspartate Aminotransferase, Cytoplasmic},{Aspartate Aminotransferase, Mitochondrial},aspartate biosynthesis,Aspartic Acid,cancer,{Carcinoma, Renal Cell},{Cell Line, Tumor},Cell Proliferation,Female,glutamine,Glutamine,GOT1,GOT2,HIF1α,Humans,{Hypoxia-Inducible Factor 1, alpha Subunit},Kidney Neoplasms,Male,Middle Aged,Mitochondrial Proteins,Neoplasms,Oxidation-Reduction,oxygen,proliferation,renal cell carcinoma,Tumor Suppressor Proteins,Von Hippel-Lindau Tumor Suppressor Protein},
  file = {/Users/will/Zotero/storage/7EJ8GB4W/2019_meléndez-rodríguez_hif1α_suppresses_tumor_cell_proliferation_through_inhibition_of_aspartate.pdf}
