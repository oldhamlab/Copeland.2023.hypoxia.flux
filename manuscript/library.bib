  keywords = {B-Lymphocytes,Carbon Isotopes,Cell Hypoxia,{Cell Line, Tumor},Cell Proliferation,Cell Survival,Citric Acid,Citric Acid Cycle,Fumarates,Glucose,Glutaminase,Glutamine,Humans,Isotope Labeling,Malates,Oxidation-Reduction,Sulfides,Thiadiazoles},
