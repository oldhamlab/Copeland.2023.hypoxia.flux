  keywords = {AMP-Activated Protein Kinases,Animals,B-Lymphocytes,Cell Line,Glycolysis,HCT116 Cells,Humans,{Hypoxia-Inducible Factor 1, alpha Subunit},Kaplan-Meier Estimate,Mice,{Mice, Transgenic},Neoplasms,Proto-Oncogene Proteins c-myc,RNA Interference,{RNA, Small Interfering},Signal Transduction},
