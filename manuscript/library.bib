  keywords = {Animals,{Cell Line, Tumor},Cell Movement,Cell Proliferation,Colonic Neoplasms,Humans,Hypoxia-Inducible Factor 1,Mice,{Mice, Nude},Neoplasm Transplantation,{Neovascularization, Pathologic},Proto-Oncogene Proteins c-myc,Vascular Endothelial Growth Factor A}
}

@article{crown2015,
