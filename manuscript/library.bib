@article{owczarek2020,
  title = {Hypoxia Increases the Rate of Renal Gluconeogenesis via Hypoxia-Inducible Factor-1-Dependent Activation of Phosphoenolpyruvate Carboxykinase Expression},
  author = {Owczarek, Aleksandra and Gieczewska, Katarzyna and Jarzyna, Robert and Jagielski, Adam K. and Kiersztan, Anna and Gruza, Andrzej and Winiarska, Katarzyna},
