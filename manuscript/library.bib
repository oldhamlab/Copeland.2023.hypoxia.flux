  keywords = {Breast Neoplasms,{Cell Line, Tumor},{Cell Transformation, Neoplastic},{Genes, myc},Humans,{Hypoxia-Inducible Factor 1, alpha Subunit},{RNA Processing, Post-Transcriptional},Transfection},
