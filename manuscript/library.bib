  pmcid = {PMC4003458},
  keywords = {Animals,Basic Helix-Loop-Helix Transcription Factors,Carbon Isotopes,Carboxylic Acids,Carcinoma; Renal Cell,Cell Line; Tumor,Citrates,Extracellular Fluid,Gas Chromatography-Mass Spectrometry,Glutamine,Humans,Magnetic Resonance Spectroscopy,Mice,Models; Biological,Oxidation-Reduction,Von Hippel-Lindau Tumor Suppressor Protein},
